sensors {
  a {
    type pose

    frame body
    targetFrame world

    topic pose_imu

    covPosition { sigma "0.01" }
    covOrientation { sigma "0.01" }

    absoluteMeasurements true

    rotation {
      used false
      yaw 0
      pitch 0
      roll 0
    }
    translation {
      used false
    }
    delay {
      used false
    }

    mestimator
    {
      name "Cauchy"
      cauchySigma2 "10"
    }
  }
  odo {
    type odometry

    frame body
    groundFrame world

    topic velodyne_points

    covPosition { sigma "0.01" }
    covOrientation { sigma "0.01" }

    absoluteMeasurements true

    wheelBase 1
    wheelRadiusLeft 1
    wheelRadiusRight 1

    assumedWheelBase 1
    assumedWheelRadiusLeft 1
    assumedWheelRadiusRight 1

    noise {
      lwVariance 0.1
      rwVariance 0.1
    }

    rotation {
      used true
      yaw 1.8
      pitch 0.0
      roll 2.5
    }
    translation {
      used true
      x 0.1
      y 0.1
      z 0.5
    }
    delay
    {
      used false
    }

    mestimator
    {
      name "Cauchy"
      cauchySigma2 "10"
    }
  }
}

model {
  Gravity {
    used false
  }

  frames body:world

  traj {
    frame body
    referenceFrame world
    initWithPoseMeasurements true
    McSensor a
    splines {
      knotsPerSecond 10
      rotSplineOrder 4
      rotFittingLambda 0.0000001
      transSplineOrder 4
      transFittingLambda 0.0000001
    }
  }
}

calibrator {
  verbose true
  timeBaseSensor a
  estimator {
    optimizer {
      maxIterations 150
    }
  }
}